{
  // Configuração dos modelos de IA utilizados pelo Task Master
  "models": {
    // Modelo principal para geração de tarefas e operações gerais
    "main": {
      "provider": "google",                    // Provedor: Google Gemini
      "modelId": "gemini-2.0-flash",          // ID do modelo: Gemini 2.0 Flash
      "maxTokens": 1048000,                   // Máximo de tokens por requisição
      "temperature": 0.2                      // Temperatura (criatividade): 0.2 = mais focado
    },
    // Modelo para pesquisas e análises complexas
    "research": {
      "provider": "google",                   // Provedor: Google Gemini
      "modelId": "gemini-2.0-flash",         // ID do modelo: Gemini 2.0 Flash
      "maxTokens": 1048000,                  // Máximo de tokens por requisição
      "temperature": 0.1                     // Temperatura mais baixa para pesquisas precisas
    },
    // Modelo de reserva caso o principal falhe
    "fallback": {
      "provider": "google",                   // Provedor: Google Gemini
      "modelId": "gemini-2.0-flash",         // ID do modelo: Gemini 2.0 Flash
      "maxTokens": 1048000,                  // Máximo de tokens por requisição
      "temperature": 0.2                     // Temperatura padrão
    }
  },
  // Configurações globais do projeto
  "global": {
    "logLevel": "info",                       // Nível de log: info (informações)
    "debug": false,                          // Modo debug: desabilitado
    "defaultNumTasks": 10,                   // Número padrão de tarefas geradas
    "defaultSubtasks": 5,                    // Número padrão de subtarefas por tarefa
    "defaultPriority": "medium",             // Prioridade padrão: média
    "projectName": "Taskmaster",             // Nome do projeto
    "ollamaBaseURL": "http://localhost:11434/api",  // URL base para Ollama (não usado)
    "bedrockBaseURL": "https://bedrock.us-east-1.amazonaws.com",  // URL base para AWS Bedrock (não usado)
    "responseLanguage": "Português",         // Idioma de resposta: Português brasileiro
    "defaultTag": "master",                  // Tag padrão: master
    "azureOpenaiBaseURL": "https://your-endpoint.openai.azure.com/",  // URL base para Azure OpenAI (não usado)
    "userId": "1234567890"                   // ID do usuário (identificação única)
  },
  // Configurações específicas para Claude Code (não usado neste projeto)
  "claudeCode": {}
}